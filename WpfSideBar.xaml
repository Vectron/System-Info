<Window x:Class="System_Info.WpfSideBar"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 xmlns:local="clr-namespace:System_Info"
			 mc:Ignorable="d" 
			 WindowStyle="None"
			 AllowsTransparency="True"
			 Background="Transparent"
			 BorderBrush="Transparent"
			 d:DesignHeight="300" d:DesignWidth="150">
	<Border BorderBrush="Black" BorderThickness="1" CornerRadius="25" Margin="1">
		<DockPanel x:Name="MainGrid" LastChildFill="False" Margin="7">
		</DockPanel>
	</Border>
</Window>